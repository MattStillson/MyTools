  <DataSets>
    <DataSet Name="JobListDS">
      <Query>
        <DataSourceName>AgentDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Job">
            <Value>=Parameters!Job.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
SJ.name AS JobName,SJ.job_id as Jobid, SJH.instance_id,
SJH.step_id AS StepID,
SJH.step_name AS StepName,
SJH.run_status AS StepStatusCode,
CASE SJH.run_status
	WHEN 0 THEN 'Failed'
	WHEN 1 THEN 'Succeeded'
	WHEN 2 THEN 'Retry'
	WHEN 3 THEN 'Canceled'
	ELSE 'Unknown'
END AS StepstatusName,
SJH.[message] AS StepMessage,
CAST(
	LEFT(CAST(SJH.run_date AS CHAR(8)),4) + '-' + SUBSTRING(CAST(SJH.run_date AS CHAR(8)),5,2) + '-' + RIGHT(CAST(SJH.run_date AS CHAR(8)),2) + ' ' + LEFT(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),2) + ':' +
	SUBSTRING(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),3,2) + ':' + RIGHT(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),2) AS DATETIME)
AS StepStartDateTime,
CAST(
	CASE WHEN LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2)&gt;='24' THEN '23' ELSE LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) END + ':' +
	SUBSTRING(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),3,2) + ':' +
	RIGHT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2)
	AS DATETIME) +
	CAST(
		LEFT(CAST(SJH.run_date AS CHAR(8)),4) + '-' + SUBSTRING(CAST(SJH.run_date AS CHAR(8)),5,2) + '-' + RIGHT(CAST(SJH.run_date AS CHAR(8)),2) + ' ' + LEFT(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),2) + ':' +
		SUBSTRING(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),3,2) + ':' +
		RIGHT(RIGHT('000000' + CAST(run_time AS VARCHAR(10)),6),2)
		AS DATETIME)
AS StepEndDateTime,
CAST(
	CASE WHEN LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2)&gt;='24' THEN '23' ELSE LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) END + ':' + SUBSTRING(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),3,2) + ':' + RIGHT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) AS DATETIME)
AS StepDuration,
CAST(
	LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) AS INT)
AS StepDurationHours,
CAST(
	SUBSTRING(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),3,2) AS INT)
AS StepDurationMinutes,
CAST(
	RIGHT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) AS INT)
AS StepDurationSeconds,
CAST(
	LEFT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) AS INT)*60*60 + CAST(SUBSTRING(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),3,2) AS INT)*60 + CAST(RIGHT(RIGHT('000000' + CAST(run_duration AS VARCHAR(10)),6),2) AS INT)
AS StepTotalDurationSeconds
into #temp
FROM
msdb.dbo.sysjobs AS SJ
INNER JOIN msdb.dbo.sysjobhistory AS SJH
	ON SJ.job_id = SJH.job_id
LEFT JOIN msdb.dbo.sysjobsteps SJS 
	ON SJ.job_id = SJS.job_id
	AND SJH.step_id = SJS.step_id
	
Where 	SJ.job_id =@Job
ORDER BY JobName DESC, StepStartDateTime DESC

select a.*,  ( select min(StepStartDateTime) as JobStartTime1 from #temp where Jobid = a.Jobid and StepID = 0 and instance_id &gt;= a.instance_id) as JobStartTime
from #temp a</CommandText>
